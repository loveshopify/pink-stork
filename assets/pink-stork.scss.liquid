  }
  .subinner .have-a-question{
    a{
      background: no-repeat;
      padding: 0;
      font-size: unset;
      text-decoration: underline;
    }
  }
  .toggle{
    font-size: 24px;
    width: 100%;
    display: block;
    padding: 18px 0;
    border-radius: .15em;
    transition: background .3s ease;
    font-weight: 600;
  }
  .toggle:focus{
    outline:none;
  }
  .faq-title p {
    font-size: 20px;
    font-weight: 600;
    text-decoration: underline;
    text-underline-position: under;
    margin-bottom: 16px
  }

  .content-with-background.row-faq-new {
    margin-top: 20px!important
  }

  .faq-new-text {
    margin-bottom: 20px
  }

  .faq-new-text p {
    font-size: 14px;
    font-weight: 500;
    margin: 0
  }

  .faq-new-text a {
    font-size: 14px;
    font-weight: 500;
    color: #f70
  }

  .description {
    margin: 2px auto 2.25em
  }

  ul.accordion {
    list-style: none;
    padding: 0
  }
  ul.sub-question-part{
    display: none;
    margin-left: 0;
    padding-left: 50px;
  }

  ul .subinner {   
    padding-left: 0;
    overflow: hidden;
    margin-left: 0;
    display: none;
    margin-top: .5em;
  }
  ul.accordion li {
    margin: .5em 0;
    list-style:none;
    /*     border-bottom: 1px solid #EEEFF0; */
  }

  a.toggle.open + ul.subinner.show{
    li{
      background-color: #FFFFFF;
      box-shadow: unset;
      border-radius: 12px;
    }
    a.toggle{
      background-color: #FFFFFF;
      color: #000;
      padding: 20px 20px;
      font-size: 20px;
      box-shadow: 0 2px 44px 0 rgba(232,232,232,0.5);
      border-radius: 12px;
    }
    a.toggle.open{
      box-shadow: unset;
    }
  }
  .subinner a {
    background-color: #FFFFFF;
    color: #000;
    padding: 20px 20px;
    font-size: 20px;
    box-shadow: 0 2px 44px 0 rgba(232,232,232,0.5);
    border-radius: 12px;
  }
  .subinner p{
    font-weight: 600;
    padding: 0 20px;
    strong{
      font-size: 21px;
    }
  }
  a.toggle.open:before,
  a.toggle:before {
    float: right!important;
    font-size: 30px;
    font-weight: 500;
  }
  a.toggle:before {
    content: '\002B';
  }
  a.toggle.open:before {
    content: "\2212";
  }
  .subinner a.toggle:before {
    content: "";
    border: solid black;
    border-width: 0 2px 2px 0;
    display: inline-block;
    padding: 4px;
    transform: rotate(45deg);
    -webkit-transform: rotate(45deg);
    position: absolute;
    right: 20px;
    top: 30px;
  }
  .subinner a.toggle.open:before {
    transform: rotate(-135deg);
    -webkit-transform: rotate(-135deg);
  }

}
